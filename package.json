{
	"name": "ai-agent-service",
	"version": "1.0.0",
	"description": "a simple ai-agent-service made by node.js, express.js",
	"main": "./dist/index.js",
	"type": "module",
	"scripts": {
		"build": "tsc",
		"start": "cross-env NODE_ENV=production node dist/index.js",
		"watch": "cross-env NODE_ENV=development tsc --watch",
		"dev": "cross-env NODE_ENV=development concurrently \"npm run watch\" \"nodemon dist/index.js\" --delay 1"
	},
	"author": "jayash saini",
	"license": "ISC",
	"devDependencies": {
		"@types/cors": "^2.8.17",
		"@types/express": "^5.0.0",
		"@types/jsonwebtoken": "^9.0.7",
		"@types/morgan": "^1.9.9",
		"@types/node": "^22.10.5",
		"cross-env": "^7.0.3",
		"dotenv-cli": "^8.0.0",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"tsx": "^4.19.2",
		"typescript": "^5.7.3"
	},
	"dependencies": {
		"concurrently": "^9.1.2",
		"cors": "^2.8.5",
		"dotenv": "^16.4.7",
		"express": "^4.21.2",
		"jsonwebtoken": "^9.0.2",
		"morgan": "^1.10.0",
		"nodemon": "^3.1.9",
		"ts-node-dev": "^2.0.0",
		"winston": "^3.17.0",
		"yarn": "^1.22.22"
	}
}
